{% set title = "Poster Session" %}
{% extends "base.html.j2" %}

{% block head %}
  <style>
    .poster-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
      gap: 2rem;
      margin: 2rem 0;
    }

    figure {
      border-radius: var(--border-radius);
      border: var(--border-width) solid var(--mute-border-color);
      margin-bottom: 0;
      overflow: auto;
      padding: 1rem;
    }

    figure > iframe {
      height: 400px;
      width: 100%;
    }

    figure > figcaption {
      color: var(--mute-color);
      margin-block: 1rem 0;
    }
  </style>
{% endblock %}

